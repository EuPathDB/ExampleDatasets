    <prop name="familyNcbiTaxonIds">31245</prop>
    <prop name="familyNameForFiles">Schistosomatidae</prop>
  </dataset>

  <dataset class="referenceStrain">
    <prop name="organismAbbrev">shaeEgypt</prop>
    <prop name="isAnnotatedGenome">true</prop>
  </dataset>


