  <dataset class="organism">
    <prop name="organismAbbrev">shaeEgypt</prop>
    <prop name="projectName">$$projectName$$</prop>
    <prop name="ncbiTaxonId">9300000003</prop>
    <prop name="publicOrganismAbbrev">shaeEgypt</prop>
    <prop name="organismFullName">Schistosoma haematobium Egypt</prop>
    <prop name="organismNameForFiles">ShaematobiumEgypt</prop>
