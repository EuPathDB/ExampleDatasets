<datasets>
  <constant name="organismAbbrev" value="bdenJEL423"/>
  <constant name="strainAbbrev" value="JEL423"/>
  <constant name="referenceStrainOrganismAbbrev" value="bdenJEL423"/>
  <constant name="familyRepOrganismAbbrev" value="bdenJEL423"/>
  <constant name="projectName" value="FungiDB"/>
  <constant name="ncbiTaxonId" value="403673"/>
  <constant name="speciesNcbiTaxonId" value="109871"/>
  <constant name="familyNcbiTaxonIds" value="100474"/>  <!-- use genus ncbiTaxonId here -->
  <constant name="genomeVersion" value="1"/>
  <constant name="soTerm" value="supercontig"/>
  <constant name="source" value="Broad"/>
  <constant name="idPrefix" value=""/>

  <dataset class="validateOrganismInfo">
    <prop name="projectName">$$projectName$$</prop>
    <prop name="organismAbbrev">$$organismAbbrev$$</prop>
    <prop name="strainAbbrev">$$strainAbbrev$$</prop>
    <prop name="ncbiTaxonId">$$ncbiTaxonId$$</prop>
    <prop name="speciesNcbiTaxonId">$$speciesNcbiTaxonId$$</prop>
    <prop name="genomeVersion">$$genomeVersion$$</prop>
  </dataset>

  <dataset class="fasta_primary_genome_sequence">
    <prop name="projectName">$$projectName$$</prop>
    <prop name="organismAbbrev">$$organismAbbrev$$</prop>
    <prop name="ncbiTaxonId">$$ncbiTaxonId$$</prop>
    <prop name="version">$$genomeVersion$$</prop>
    <prop name="name">$$source$$</prop>
    <prop name="soTerm">$$soTerm$$</prop>
    <prop name="table">DoTS::ExternalNASequence</prop>
    <prop name="sourceIdRegex">^>(\S+)</prop>
    <prop name="releaseDate">2007-03-01</prop>
  </dataset>

  <dataset class="NoPreprocess_primary_genome_features">
    <prop name="projectName">$$projectName$$</prop>
    <prop name="organismAbbrev">$$organismAbbrev$$</prop>
    <prop name="ncbiTaxonId">$$ncbiTaxonId$$</prop>
    <prop name="version">$$genomeVersion$$</prop>
    <prop name="source">$$source$$</prop>
    <prop name="soTerm">$$soTerm$$</prop>
    <prop name="mapFile">genemRNAExonCDS2Gus.xml</prop>
  </dataset>

  <dataset class="fasta_organelle_genome">
    <prop name="projectName">$$projectName$$</prop>
    <prop name="organismAbbrev">$$organismAbbrev$$</prop>
    <prop name="ncbiTaxonId">$$ncbiTaxonId$$</prop>
    <prop name="version">$$genomeVersion$$</prop>
    <prop name="name">$$source$$</prop>
    <prop name="soTerm">mitochondrial_chromosome</prop>
    <prop name="table">DoTS::ExternalNASequence</prop>
    <prop name="sourceIdRegex">^>(\S+)</prop>
    <prop name="organelle">mitochondrion</prop>
  </dataset>

  <dataset class="productNames">
    <prop name="projectName">$$projectName$$</prop>
    <prop name="organismAbbrev">$$organismAbbrev$$</prop>
    <prop name="version">$$genomeVersion$$</prop>
    <prop name="name">$$source$$</prop>
  </dataset>

  <dataset class="aliases">
    <prop name="name">PreviousAltSplGeneIds</prop>
    <prop name="version">2016-05-13</prop>
    <prop name="idType">previous id</prop>
    <prop name="projectName">$$projectName$$</prop>
    <prop name="organismAbbrev">$$organismAbbrev$$</prop>
    <prop name="columnSpec">primary_identifier</prop>
    <prop name="url"></prop>
    <prop name="urlUsesSecondaryId"></prop>
    <prop name="target">NAFeature</prop>
    <prop name="viewName">GeneFeature</prop>
  </dataset>

  <dataset class="dbxref_gene2Entrez">
    <prop name="projectName">$$projectName$$</prop>
    <prop name="organismAbbrev">$$organismAbbrev$$</prop>
    <prop name="ncbiTaxonId">$$ncbiTaxonId$$</prop>
    <prop name="genomeVersion">$$genomeVersion$$</prop>
    <prop name="version">2016-05-05</prop>
  </dataset>

  <dataset class="dbxref_gene2Uniprot">
    <prop name="projectName">$$projectName$$</prop>
    <prop name="organismAbbrev">$$organismAbbrev$$</prop>
    <prop name="ncbiTaxonId">$$ncbiTaxonId$$</prop>
    <prop name="genomeVersion">$$genomeVersion$$</prop>
    <prop name="version">2016-05-05</prop>
  </dataset>

  <dataset class="dbxref_gene2PubmedFromNcbi">
    <prop name="projectName">$$projectName$$</prop>
    <prop name="organismAbbrev">$$organismAbbrev$$</prop>
    <prop name="ncbiTaxonId">$$ncbiTaxonId$$</prop>
    <prop name="genomeVersion">$$genomeVersion$$</prop>
    <prop name="version">2016-05-05</prop>
  </dataset>

  <dataset class="ECAssocFromUniprot">
    <prop name="speciesNcbiTaxonId">$$speciesNcbiTaxonId$$</prop>
    <prop name="organismAbbrev">$$organismAbbrev$$</prop>
    <prop name="version">TODAY</prop>
  </dataset> 

  <dataset class="transcriptsFromReferenceStrain">
    <prop name="referenceStrainOrganismAbbrev">$$referenceStrainOrganismAbbrev$$</prop>
  </dataset>
  <dataset class="epitopesFromReferenceStrain">
    <prop name="referenceStrainOrganismAbbrev">$$referenceStrainOrganismAbbrev$$</prop>
  </dataset>

  <dataset class="referenceStrain-dbEST">
    <prop name="projectName">$$projectName$$</prop>
    <prop name="organismAbbrev">$$organismAbbrev$$</prop>
    <prop name="speciesNcbiTaxonId">$$speciesNcbiTaxonId$$</prop>
  </dataset>

  <dataset class="referenceStrain-epitope_sequences_IEDB">
    <prop name="organismAbbrev">$$organismAbbrev$$</prop>
    <prop name="speciesNcbiTaxonId">$$speciesNcbiTaxonId$$</prop>
    <prop name="version">2.4</prop>
  </dataset>

  <dataset class="referenceStrain-ESTsFromFasta">
    <prop name="projectName">$$projectName$$</prop>
    <prop name="organismAbbrev">$$organismAbbrev$$</prop>
    <prop name="ncbiTaxonId">$$ncbiTaxonId$$</prop>
    <prop name="name">JGI_Bden</prop>
    <prop name="version">1</prop>
    <prop name="sourceIdRegex">&gt;(\S+)$</prop>
    <prop name="soTerm">EST</prop>
    <prop name="isPutativeFullLength">false</prop>
    <prop name="libraryVector">pMCL200_cDNA</prop>
    <prop name="libraryStage">mixed zoospores and sporangia</prop>
    <prop name="libraryName">LIBEST_CBWH_Batrachochytrium_+dendrobatidis</prop>
    <prop name="libraryDesc">In Vivo Conditions: B.dendrobatidis zoospore and sporangia mixed culture. The library was made from oligo(dT)-primed cDNA and cloned into the pMCL200_cDNA vector. PolyA RNA was primed with oligo(dT) primer (5'-GACTAGTTCTAGATCGCGAGCGGCCGCCCTTTTTTTTTTTTTTT-3'), ligated to a SalI adapter (5'-TCGACCCACGCGTCCG and 5'-CGGACGCGTGGG) and digested with NotI. cDNA was size selected from &gt;1.2-2kb using 1.1% agarose gel electrophoresis then ligated into NotI and SalI-digested pMCL200_cDNA vector </prop>
    <prop name="contactName">JGI</prop>
    <prop name="contactAddress1">DOE Joint Genome Institute</prop>
    <prop name="contactAddress2">2800 Mitchell Drive, Walnut Creek, CA 94598, USA</prop>
  </dataset>

  <dataset class="isolatesFromFamilyRepresentative">
    <prop name="name">genbank</prop>
    <prop name="familyRepOrganismAbbrev">$$familyRepOrganismAbbrev$$</prop>
  </dataset>

  <dataset class="familyRepresentative-isolatesGenbank">
    <prop name="organismAbbrev">$$organismAbbrev$$</prop>
    <prop name="ncbiTaxonId">$$familyNcbiTaxonIds$$</prop>
  </dataset>
</datasets>

